package Test;

import action.Pince;

public class TestFermerPince {

    public static void main(String[] args) {
        // Instanciation de la classe Pince
        Pince pince = new Pince();

        System.out.println("Test : Fermeture de la pince...");
        pince.fermerPince(); // Appel de la méthode pour fermer la pince
        System.out.println("Pince fermée avec succès.");
    }
}
